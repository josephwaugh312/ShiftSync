{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!static|favicon.ico|icon|manifest.json|sw.js).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
} 